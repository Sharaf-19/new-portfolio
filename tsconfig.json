{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "target": "es6",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
